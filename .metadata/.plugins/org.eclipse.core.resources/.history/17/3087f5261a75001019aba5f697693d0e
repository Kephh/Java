package com.fizzbuzz.app;

//import java.util.Scanner;

class FizzBuzz{
	public static void main(String args[]) {
//		Scanner sc = new Scanner(System.in);
//		int lim = sc.nextInt();
		int fizzCount = 0;
		int buzzCount = 0;
		for(int i = 1; i <= 100; i++) {
			Boolean flag = false;
			if(i % 3 == 0) {
				System.out.print("Fizz");
				fizzCount ++;
				flag = true;
			}
			if(i % 5 == 0) {
				System.out.print("Buzz");
				buzzCount ++;
				flag = true;
			}
			if(flag)
				System.out.println("");
		}
		System.out.println("Fizz count: "+fizzCount+", Buzz count: "+buzzCount);
		System.out.print("FizzBuzz count: "+(fizzCount + buzzCount)+", Numbers count: "+(100 - fizzCount - buzzCount));
	}
}