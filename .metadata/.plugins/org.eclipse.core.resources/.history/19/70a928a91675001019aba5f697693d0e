package com.fizzbuzz.app;

class FizzBuzz{
	public static void main(String args[]) {
		for(int i = 1; i < 100; i++) {
			Boolean flag = false;
			if(i % 3 == 0) {
				System.out.print("Fizz");
				flag = true;
			}
			if(i % 5 == 0) {
				System.out.print("Buzz");
				flag = true;
			}
			if(flag)
				System.out.println("");
		}
	}
}